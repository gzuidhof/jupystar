{
    "name": "jupystar",
    "version": "0.1.0",
    "description": "Converter from Jupyter notebook (ipynb) to Starboard notebook",
    "keywords": [
        "notebook",
        "jupyter",
        "starboard",
        "starboard-notebook"
    ],
    "main": "dist/index.js",
    "module": "dist/index.js",
    "author": "Guido Zuidhof <me@guido.io>",
    "repository": {
        "type": "git",
        "url": "github.com/gzuidhof/jupystar"
    },
    "license": "MIT",
    "scripts": {
        "build": "rimraf dist && rollup -c rollup.config.ts",
        "prepublishOnly": "npm run build && tsc"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^16.0.0",
        "@rollup/plugin-node-resolve": "^10.0.0",
        "express": "^4.17.1",
        "jsesc": "^3.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.33.1",
        "rollup-plugin-typescript2": "^0.29.0",
        "typescript": "^4.0.5"
    },
    "dependencies": {
        "@rollup/plugin-replace": "^2.3.4",
        "starboard-notebook": "^0.6.2"
    }
}
